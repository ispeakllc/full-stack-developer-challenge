{
  "name": "@nx-ddd/source",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "ui-add": "pnpm dlx shadcn@latest add",
    "live:dev": "NX_TUI=false sst dev --stage development",
    "live:staging": "NX_TUI=false sst dev --stage staging",
    "live:production": "NX_TUI=false sst dev --stage production",
    "typecheck:workspace": "nx run-many --target typecheck --all"
  },
  "nx": {
    "name": "root",
    "implicitDependencies": [
      "web",
      "events-processor"
    ],
    "targets": {
      "deploy": {
        "executor": "nx:run-commands",
        "dependsOn": [
          "^build",
          "^open-next-build"
        ],
        "options": {
          "command": "sst deploy --stage {args.stage}",
          "forwardAllArgs": true
        },
        "defaultConfiguration": "dev",
        "configurations": {
          "production": {
            "args": [
              "--stage=production"
            ]
          },
          "staging": {
            "args": [
              "--stage=staging"
            ]
          },
          "dev": {
            "args": [
              "--stage=development"
            ]
          }
        }
      },
      "refresh": {
        "executor": "nx:run-commands",
        "options": {
          "command": "sst refresh --stage {args.stage}",
          "forwardAllArgs": true
        },
        "defaultConfiguration": "dev",
        "configurations": {
          "production": {
            "args": [
              "--stage=production"
            ]
          },
          "staging": {
            "args": [
              "--stage=staging"
            ]
          },
          "dev": {
            "args": [
              "--stage=development"
            ]
          }
        }
      },
      "tunnel": {
        "executor": "nx:run-commands",
        "options": {
          "command": "sst tunnel --stage {args.stage}",
          "forwardAllArgs": true
        },
        "defaultConfiguration": "dev",
        "configurations": {
          "production": {
            "args": [
              "--stage=production"
            ]
          },
          "staging": {
            "args": [
              "--stage=staging"
            ]
          },
          "dev": {
            "args": [
              "--stage=development"
            ]
          }
        }
      },
      "destroy": {
        "executor": "nx:run-commands",
        "options": {
          "command": "sst remove --stage {args.stage}",
          "forwardAllArgs": true
        },
        "defaultConfiguration": "dev",
        "configurations": {
          "production": {
            "args": [
              "--stage=production"
            ]
          },
          "staging": {
            "args": [
              "--stage=staging"
            ]
          },
          "dev": {
            "args": [
              "--stage=development"
            ]
          }
        }
      },
      "local-registry": {
        "executor": "@nx/js:verdaccio",
        "options": {
          "port": 4873,
          "config": ".verdaccio/config.yml",
          "storage": "tmp/local-registry/storage"
        }
      }
    }
  },
  "private": true,
  "dependencies": {
