{
  "name": "@nx-ddd/job-events-infra",
  "version": "0.0.1",
  "private": true,
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "development": "./src/index.ts",
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    },
    "./env": "./src/env.mjs"
  },
  "nx": {
    "name": "job-events-infra",
    "sourceRoot": "libs/jobs/events/infrastructure/src",
    "targets": {
      "build": {
        "executor": "@nx/js:swc",
        "outputs": [
          "{options.outputPath}"
        ],
        "options": {
          "outputPath": "libs/jobs/events/infrastructure/dist",
          "main": "libs/jobs/events/infrastructure/src/index.ts",
          "tsConfig": "libs/jobs/events/infrastructure/tsconfig.lib.json",
          "skipTypeCheck": true,
          "stripLeadingPaths": true,
          "assets": [
            {
              "glob": "env.mjs",
              "input": "libs/jobs/events/infrastructure/src",
              "output": "."
            }
          ]
        }
      }
    }
  },
  "dependencies": {
    "@nx-ddd/job-events-domain": "workspace:*",
    "@nx-ddd/email-domain": "workspace:*",
    "@nx-ddd/email-application": "workspace:*",
    "@nx-ddd/job-events-presentation": "workspace:*",
    "@nx-ddd/email-infrastructure": "workspace:*",
    "@nestjs/microservices": "^11.1.5",
    "@nestjs/cqrs": "catalog:",
    "@swc/helpers": "~0.5.11",
    "@t3-oss/env-nextjs": "^0.13.6"
  }
}
