{
  "name": "@nx-ddd/job-events-presentation",
  "version": "0.0.1",
  "private": true,
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "development": "./src/index.ts",
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    }
  },
  "nx": {
    "name": "job-events-presentation",
    "sourceRoot": "libs/jobs/events/presentation/src",
    "targets": {
      "build": {
        "executor": "@nx/js:swc",
        "outputs": [
          "{options.outputPath}"
        ],
        "options": {
          "outputPath": "libs/jobs/events/presentation/dist",
          "main": "libs/jobs/events/presentation/src/index.ts",
          "tsConfig": "libs/jobs/events/presentation/tsconfig.lib.json",
          "skipTypeCheck": true,
          "stripLeadingPaths": true
        }
      }
    }
  },
  "dependencies": {
    "@nx-ddd/job-events-domain": "workspace:*",
    "@nx-ddd/email-application": "workspace:*",
    "@nx-ddd/email-domain": "workspace:*",
    "@nestjs/microservices": "^11.1.5",
    "@swc/helpers": "~0.5.11"
  }
}