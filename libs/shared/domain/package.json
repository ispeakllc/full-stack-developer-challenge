{
  "name": "@nx-ddd/shared-domain",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "development": "./src/index.ts",
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    }
  },
  "nx": {
    "name": "shared-domain",
    "targets": {
      "build": {
        "executor": "@nx/js:swc",
        "outputs": [
          "{options.outputPath}"
        ],
        "options": {
          "outputPath": "libs/shared/domain/dist",
          "main": "libs/shared/domain/src/index.ts",
          "tsConfig": "libs/shared/domain/tsconfig.lib.json",
          "stripLeadingPaths": true
        }
      }
    }
  },
  "dependencies": {
    "zod": "catalog:zod3",
    "@nestjs/common": "catalog:nestjs",
    "@nestjs/cqrs": "catalog:nestjs",
    "uuid": "catalog:",
    "@swc/helpers": "~0.5.11"
  }
}
