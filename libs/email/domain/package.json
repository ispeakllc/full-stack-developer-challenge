{
  "name": "@nx-ddd/email-domain",
  "version": "0.0.1",
  "private": true,
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "development": "./src/index.ts",
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    }
  },
  "nx": {
    "name": "email-domain",
    "sourceRoot": "libs/email/domain/src",
    "targets": {
      "build": {
        "executor": "@nx/js:swc",
        "outputs": [
          "{options.outputPath}"
        ],
        "options": {
          "outputPath": "libs/email/domain/dist",
          "main": "libs/email/domain/src/index.ts",
          "tsConfig": "libs/email/domain/tsconfig.lib.json",
          "skipTypeCheck": true,
          "stripLeadingPaths": true
        }
      }
    }
  },
  "dependencies": {
    "@swc/helpers": "~0.5.11",
    "@nx-ddd/shared-domain": "workspace:*",
    "@nx-ddd/job-events-domain": "workspace:*",
    "zod": "catalog:"
  },
  "devDependencies": {
    "@nestjs-modules/mailer": "^2.0.2"
  }
}
