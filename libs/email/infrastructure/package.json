{
  "name": "@nx-ddd/email-infrastructure",
  "version": "0.2.0",
  "private": true,
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "types": "./src/index.ts",
      "import": "./src/index.ts",
      "default": "./src/index.ts",
      "development": "./src/index.ts"
    },
    "./env": {
      "development": "./src/env.ts",
      "types": "./dist/env.d.ts",
      "import": "./dist/env.js",
      "default": "./dist/env.js"
    }
  },
  "nx": {
    "name": "email-infrastructure",
    "targets": {
      "build": {
        "executor": "@nx/js:swc",
        "outputs": [
          "{options.outputPath}"
        ],
        "options": {
          "outputPath": "libs/email/infrastructure/dist",
          "main": "libs/email/infrastructure/src/index.ts",
          "tsConfig": "libs/email/infrastructure/tsconfig.lib.json",
          "stripLeadingPaths": true
        }
      }
    }
  },
  "dependencies": {
    "@aws-sdk/client-sesv2": "^3.849.0",
    "@nx-ddd/email-application": "workspace:*",
    "@nx-ddd/email-domain": "workspace:*",
    "@nx-ddd/shared-domain": "workspace:*",
    "@react-email/components": "catalog:",
    "nodemailer": "^7.0.5"
  },
  "devDependencies": {
    "@types/nodemailer": "^6.4.17"
  }
}
