{
  "name": "@nx-ddd/auth-infrastructure",
  "version": "0.0.1",
  "private": true,
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "development": "./src/index.ts",
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    },
    "./env": "./src/env.mjs"
  },
  "nx": {
    "name": "auth-infrastructure",
    "sourceRoot": "libs/auth/infrastructure/src",
    "targets": {
      "build": {
        "executor": "@nx/js:swc",
        "outputs": [
          "{options.outputPath}"
        ],
        "options": {
          "outputPath": "libs/auth/infrastructure/dist",
          "main": "libs/auth/infrastructure/src/index.ts",
          "tsConfig": "libs/auth/infrastructure/tsconfig.lib.json",
          "skipTypeCheck": true,
          "stripLeadingPaths": true
        }
      }
    }
  },
  "dependencies": {
    "@react-email/components": "^0.3.2",
    "@nx-ddd/auth-application": "workspace:*",
    "@nx-ddd/auth-domain": "workspace:*",
    "@nx-ddd/user-domain": "workspace:*",
    "@nx-ddd/database-infrastructure": "workspace:*",
    "@swc/helpers": "~0.5.11",
    "zod": "catalog:",
    "@t3-oss/env-nextjs": "^0.13.6"
  }
}
