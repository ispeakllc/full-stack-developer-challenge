{
  "name": "@nx-ddd/post-infrastructure",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "development": "./src/index.ts",
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    }
  },
  "nx": {
    "name": "post-infrastructure",
    "targets": {
      "build": {
        "executor": "@nx/js:swc",
        "outputs": [
          "{options.outputPath}"
        ],
        "options": {
          "outputPath": "libs/post/infrastructure/dist",
          "main": "libs/post/infrastructure/src/index.ts",
          "tsConfig": "libs/post/infrastructure/tsconfig.lib.json",
          "stripLeadingPaths": true
        }
      }
    }
  },
  "dependencies": {
    "@nestjs/common": "catalog:nestjs",
    "@nx-ddd/database-infrastructure": "workspace:*",
    "@nx-ddd/post-application": "workspace:*",
    "@nx-ddd/post-domain": "workspace:*",
    "@nx-ddd/post-presentation": "workspace:*",
    "@nx-ddd/shared-domain": "workspace:*",
    "@nx-ddd/user-domain": "workspace:^",
    "@nx-ddd/user-infrastructure": "workspace:*",
    "@swc/helpers": "~0.5.11"
  },
  "devDependencies": {
    "@nx-ddd/auth-domain": "workspace:*",
    "@nx-ddd/database-application": "workspace:*",
    "@nx-ddd/shared-infrastructure": "workspace:*",
    "unplugin-swc": "^1.5.5",
    "vite-plugin-node": "^7.0.0"
  }
}
