{
  "expo": {
    "name": "skyrim-inventory",
    "slug": "skyrim-inventory",
    "scheme": "expo",
    "version": "0.1.0",
    "orientation": "portrait",
    "icon": "./assets/skyrim-logo-icon.png",
    "userInterfaceStyle": "automatic",
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "newArchEnabled": true,
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "bundleIdentifier": "your.bundle.identifier",
      "supportsTablet": true,
      "icon": {
        "light": "./assets/skyrim-logo-icon.png",
        "dark": "./assets/skyrim-logo-icon.png"
      }
    },
    "android": {
      "package": "your.bundle.identifier",
      "adaptiveIcon": {
        "foregroundImage": "./assets/skyrim-logo-icon.png",
        "backgroundColor": "#1F104A"
      },
      "edgeToEdgeEnabled": true
    },
    "experiments": {
      "tsconfigPaths": true,
      "typedRoutes": true
    },
    "plugins": [
      "expo-router",
      "expo-secure-store",
      "expo-web-browser",
      [
        "expo-splash-screen",
        {
          "backgroundColor": "#18181B",
          "image": "./assets/skyrim-logo.png",
          "dark": {
            "backgroundColor": "#18181B",
            "image": "./assets/skyrim-logo.png"
          }
        }
      ]
    ],
    "extra": {
      "router": {},
      "eas": {
        "projectId": "ae9224da-ebe1-4581-971a-d527d23f5b34"
      }
    }
  }
}