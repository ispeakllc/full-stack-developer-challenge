{
  "name": "@nx-ddd/web",
  "version": "0.0.1",
  "private": true,
  "nx": {
    "name": "web",
    "targets": {
      "open-next-build": {
        "executor": "nx:run-commands",
        "dependsOn": [
          "build"
        ],
        "cache": true,
        "outputs": [
          "{projectRoot}/.open-next"
        ],
        "options": {
          "cwd": "apps/web",
          "command": "open-next build"
        }
      }
    }
  },
  "dependencies": {
    "next": "~15.2.4",
    "react": "19.0.0",
    "next-safe-action": "^7.10.8",
    "react-dom": "19.0.0",
    "jiti": "^2.4.2",
    "@nx-ddd/api-infrastructure": "workspace:*",
    "@nx-ddd/auth-domain": "workspace:*",
    "@nx-ddd/shared-infrastructure": "workspace:*"
  },
  "devDependencies": {
    "@opennextjs/aws": "^3.7.0"
  }
}