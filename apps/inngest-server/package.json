{
  "name": "@nx-ddd/inngest-server",
  "version": "0.0.1",
  "private": true,
  "nx": {
    "name": "inngest-server",
    "targets": {
      "build": {
        "executor": "nx:run-commands",
        "options": {
          "command": "webpack-cli build",
          "args": [
            "--node-env=production"
          ]
        },
        "configurations": {
          "development": {
            "args": [
              "--node-env=development"
            ]
          }
        }
      },
      "serve": {
        "continuous": true,
        "executor": "@nx/js:node",
        "defaultConfiguration": "development",
        "dependsOn": [
          "build"
        ],
        "options": {
          "buildTarget": "inngest-server:build",
          "runBuildTargetDependencies": false
        },
        "configurations": {
          "development": {
            "buildTarget": "inngest-server:build:development"
          },
          "production": {
            "buildTarget": "inngest-server:build:production"
          }
        }
      }
    }
  },
  "scripts": {
    "dev": "nest start --watch"
  },
  "dependencies": {
    "@nestjs/platform-fastify": "^11.1.5",
    "@nx-ddd/jobs-events-presentation": "workspace:*",
    "@nx-ddd/email-infrastructure": "workspace:*",
    "inngest": "3.40.1",
    "nest-inngest": "1.3.2"
  }
}
