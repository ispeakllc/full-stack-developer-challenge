{
  "version": 2,
  "buildCommand": "cd ../../ && pnpm nx run inngest-server:build",
  "functions": {
    "main.js": {
      "includeFiles": "dist/**/*"
    }
  },
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/main.js"
    }
  ],
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "outputDirectory": "dist"
}