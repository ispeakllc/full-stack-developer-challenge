{
  "version": 2,
  "buildCommand": "cd ../../ && pnpm nx run inngest-server:build",
  "builds": [
    {
      "src": "dist/main.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "dist/main.js",
      "methods": [
        "GET",
        "POST",
        "PUT",
        "DELETE",
        "PATCH",
        "OPTIONS"
      ]
    }
  ],
  "$schema": "https://openapi.vercel.sh/vercel.json"
}