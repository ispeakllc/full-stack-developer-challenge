{
  "name": "@nx-ddd/zoddd",
  "version": "0.0.1",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "development": "./src/index.ts",
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "!**/*.tsbuildinfo",
    "generators.json"
  ],
  "nx": {
    "name": "zoddd",
    "sourceRoot": "tools/zoddd/src",
    "targets": {
      "build": {
        "executor": "@nx/js:swc",
        "outputs": [
          "{options.outputPath}"
        ],
        "options": {
          "outputPath": "tools/zoddd/dist",
          "main": "tools/zoddd/src/index.ts",
          "tsConfig": "tools/zoddd/tsconfig.lib.json",
          "skipTypeCheck": true,
          "stripLeadingPaths": true,
          "assets": [
            {
              "input": "./tools/zoddd/src",
              "glob": "**/!(*.ts)",
              "output": "."
            },
            {
              "input": "./tools/zoddd/src",
              "glob": "**/*.d.ts",
              "output": "."
            }
          ]
        }
      }
    }
  },
  "dependencies": {
    "@nx/devkit": "21.3.1",
    "@swc/helpers": "~0.5.11"
  },
  "generators": "./generators.json"
}
