{
  "name": "@nx-ddd/tailwind",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "development": "./src/index.ts",
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    }
  },
  "nx": {
    "name": "tailwind",
    "targets": {
      "build": {
        "executor": "@nx/js:swc",
        "outputs": [
          "{options.outputPath}"
        ],
        "options": {
          "outputPath": "tools/tailwind/dist",
          "main": "tools/tailwind/src/index.ts",
          "tsConfig": "tools/tailwind/tsconfig.lib.json",
          "stripLeadingPaths": true
        }
      }
    }
  },
  "dependencies": {
    "tailwindcss": "catalog:tailwind3",
    "tailwindcss-animate": "catalog:",
    "@swc/helpers": "~0.5.11"
  }
}
